{
  "name": "@nsidc/search-interface",
  "description": "NSIDC data search interface",
  "version": "3.0.2-5",
  "license": "GPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "https://github.com/nsidc/search-interface.git"
  },
  "files": [
    "dist",
    "COPYING",
    "CHANGELOG",
    "LICENSE",
    "README.md"
  ],
  "main": "dist/index.bundle.js",
  "scripts": {
    "build:prod": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "start": "NODE_ENV=development webpack serve --progress",
    "clean": "rm -rf ./dist/*",
    "doc": "jsdoc -c jsdoc-conf.json",
    "lint": "npx eslint ./src/",
    "lint:watch": "npx esw -w ./src/",
    "test": "jest ./src",
    "test:watch": "jest -w ./src",
    "release": "release-it"
  },
  "engines": {
    "node": "^14.17.0",
    "npm": ">=6.14"
  },
  "release-it": {
    "npm": {
      "publish": false
    },
    "plugins": {
      "@j-ulrich/release-it-regex-bumper": {
        "out": {
          "file": "CHANGELOG.md",
          "search": "UNRELEASED",
          "replace": "Version {{version}}"
        }
      }
    }
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.8",
    "@j-ulrich/release-it-regex-bumper": "^2.0.0",
    "OpenSearchlight": "https://github.com/nsidc/OpenSearchlight.git#soac-62",
    "auto-changelog": "^2.3.0",
    "backbone": "^1.4.0",
    "corejs-typeahead": "^1.3.1",
    "css-loader": "^5.2.6",
    "date-fns": "^2.17.0",
    "eslint": "^7.31.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-watch": "^7.0.0",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "html-webpack-template": "^6.2.0",
    "http-server": "^0.12.3",
    "jasmine-core": "^3.7.0",
    "jasmine-sinon": "0.4.0",
    "jest": "^27.0.6",
    "jest-cli": "^27.0.6",
    "jest-junit": "^12.2.0",
    "jquery": "^3.5.1",
    "jsdoc": "^3.6.7",
    "karma": "^6.3.3",
    "karma-chrome-launcher": "3.1.0",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-jquery-2": "0.1.1",
    "karma-jasmine-sinon": "1.0.4",
    "karma-moment": "0.1.0",
    "karma-sinon": "1.0.5",
    "karma-spec-reporter": "0.0.32",
    "leaflet": "^1.7.1",
    "node-gyp": "^8.1.0",
    "npm-audit-resolver": "^2.3.1",
    "ol": "^6.6.1",
    "popper.js": "^1.16.1",
    "proj4": "^2.6.3",
    "release-it": "^14.10.0",
    "sass": "^1.32.5",
    "sass-loader": "^12.0.0",
    "sinon": "^11.1.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.6.1",
    "tippy.js": "^6.3.0",
    "typescript": "^4.2.4",
    "typescript-language-server": "^0.5.1",
    "underscore": "^1.12.0",
    "url-loader": "^4.1.1",
    "vanillajs-datepicker": "^1.1.4",
    "voca": "^1.4.0",
    "webpack": "^5.45.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.4.1",
    "xregexp": "^5.0.1"
  }
}
