{
  "name": "@nsidc/search-interface",
  "description": "NSIDC data search interface",
  "version": "4.7.0",
  "license": "GPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "https://github.com/nsidc/search-interface.git"
  },
  "homepage": "https://github.com/nsidc/search-interface",
  "files": [
    "dist",
    "COPYING",
    "CHANGELOG",
    "LICENSE",
    "README.md"
  ],
  "type": "module",
  "browser": "./dist/index.bundle.js",
  "scripts": {
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "start": "webpack serve --node-env development --no-client-overlay --progress",
    "clean": "rm -rf ./dist/*",
    "doc": "jsdoc -c jsdoc-conf.json",
    "lint": "npx eslint ./src/",
    "lint:watch": "npx esw -w ./src/",
    "test": "jest src/spec",
    "testbroken": "jest src/broken_spec/lib src/broken_spec/models src/broken_spec/views",
    "test:watch": "jest -w src/spec/lib/utility_functions_spec.js src/spec/lib/Mediator_spec.js src/spec/lib/NsidcOpenSearchResponse_spec.js src/spec/collections",
    "setup:prerelease": "npm version prepatch --no-git-tag-version; npm version prerelease --preid=rc --no-git-tag-version",
    "bump:prerelease": "npm version prerelease --preid=rc",
    "release": "npm version",
    "version": "./release.sh",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
    "backbone": "^1.4.0",
    "corejs-typeahead": "^1.3.1",
    "date-fns": "^2.17.0",
    "jquery": "^3.5.1",
    "jquery-ui": "^1.12.1",
    "leaflet": "^1.7.1",
    "ol": "^7.4.0",
    "opensearch-browser": "^2.0.0-alpha.7",
    "proj4": "^2.8.0",
    "tippy.js": "^6.3.7",
    "underscore": "^1.13.6",
    "vanillajs-datepicker": "^1.2.0",
    "voca": "^1.4.0",
    "xregexp": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.14.8",
    "@testing-library/jest-dom": "^6.1.3",
    "auto-changelog": "^2.3.0",
    "babel-jest": "^29.6.2",
    "css-loader": "^6.7.1",
    "eslint": "^8.45.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-watch": "^8.0.0",
    "follow-redirects": "^1.14.7",
    "html-loader": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "http-server": "^14.0.0",
    "imports-loader": "^4.0.0",
    "jasmine-core": "^5.0.0",
    "jasmine-sinon": "0.4.0",
    "jest": "^29.6.2",
    "jest-cli": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "jest-html-loader": "^1.0.0",
    "jest-junit": "^16.0.0",
    "karma": "^6.3.3",
    "karma-chrome-launcher": "^3.0.0",
    "karma-jasmine": "^5.0.0",
    "karma-jasmine-jquery-2": "0.1.1",
    "karma-jasmine-sinon": "1.0.4",
    "karma-sinon": "1.0.5",
    "karma-spec-reporter": "~0.0.32",
    "log4js": "^6.4.4",
    "minimist": "^1.2.6",
    "node-forge": "^1.3.1",
    "node-gyp": "^9.4.0",
    "npm-audit-resolver": "^3.0.0-6",
    "postcss": "^8.4.12",
    "resolve-url-loader": "^5.0.0",
