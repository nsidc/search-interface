{
  "clearMocks": true,
  "moduleDirectories": ["node_modules", "src"],
  "moduleFileExtensions": ["js", "jsx"],
  "modulePaths": ["<rootDir>/src"],
  "rootDir": "src",
  "testEnvironment": "jsdom",
  "testRegex": "((\\.|/*.)(spec))\\.js?$",
  "transform": {
    "^.+\\.(js|jsx)$": ["babel-jest", {"rootMode": "upward"}]
  },
  "verbose": true
}
