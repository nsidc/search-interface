<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/" xmlns:essi="http://essi-lab.eu" xmlns:time="http://a9.com/-/opensearch/extensions/time/1.0/" xmlns:geo="http://a9.com/-/opensearch/extensions/geo/1.0/" xmlns:moz="http://www.mozilla.org/2006/browser/search/" xmlns:gvq="http://www.geoviqua.org/schemas/ProducerModel/2.2">
	  <ShortName>GI-cat</ShortName>
	  <LongName>GI-cat OpenSearch Catalog service</LongName>
	  <Description>GI-cat interface for the OpenSearch support.
	*** Note about the OpenSearch plugin for Firefox toolbar ***. 
	Url templates with "text/html" type are required in order to enable the Firefox plugin ( at least one is required ) but the actual returned mimetype 
	is always GeoAtom. For more info see https://developer.mozilla.org/en/Creating_OpenSearch_plugins_for_Firefox 
	</Description>
	  <Contact>fabrizio.papeschi@cnr.it</Contact>
	  <Image height="16" type="image/x-icon" width="16">http://zeus.pin.unifi.it/projects/gi-cat-favicon/favicon.ico</Image>
	  <Url template="/api/gi-cat/services/opensearch?si={startIndex?}&amp;ct={count?}&amp;st={searchTerms?}&amp;bbox={geo:box?}&amp;rel={geo:relation?}&amp;loc={geo:locationString?}&amp;ts={time:start?}&amp;te={time:end?}&amp;rdt={gvq:refDocTitle?}&amp;rv={gvq:resValue?}&amp;rvu={gvq:resValueUnit?}&amp;psc={gvq:prcStepCount?}&amp;wa={gvq:workAround?}&amp;lac={essi:legalAccConst?}&amp;luc={essi:legalUseConst?}&amp;legalOp={essi:legalOperator?}&amp;hl={essi:hierarchyLevel?}&amp;outputFormat=text/html" type="text/html"/>	
	  <Url template="/api/gi-cat/services/opensearch?si={startIndex?}&amp;ct={count?}&amp;st={searchTerms?}&amp;bbox={geo:box?}&amp;rel={geo:relation?}&amp;loc={geo:locationString?}&amp;ts={time:start?}&amp;te={time:end?}&amp;rdt={gvq:refDocTitle?}&amp;rv={gvq:resValue?}&amp;rvu={gvq:resValueUnit?}&amp;psc={gvq:prcStepCount?}&amp;wa={gvq:workAround?}&amp;lac={essi:legalAccConst?}&amp;luc={essi:legalUseConst?}&amp;legalOp={essi:legalOperator?}&amp;hl={essi:hierarchyLevel?}&amp;outputFormat=application/atom+xml" type="application/atom+xml"/>
	  <Query geo:box="-180,-90,180,90" role="example"/>
	  <Query geo:box="-20,-10,10,20" geo:relation="overlaps" role="example"/>
	  <Query geo:box="-20,-10,10,20" geo:relation="contains" role="example"/>
	  <Query geo:box="-20,-10,10,20" geo:relation="disjoint" role="example"/>
	  <Query geo:locationString="italy" role="example"/>
	  <Query role="example" time:end="2010-01-01T23:39:59" time:start="1990-01-01T00:00:00"/>
	  <Developer>fabrizio.papeschi@cnr.it</Developer>
	  <Developer>enrico.boldrini@cnr.it</Developer>
	  <SyndicationRight>open</SyndicationRight>
	  <AdultContent>false</AdultContent>
	  <Language>en-us</Language>
	  <OutputEncoding>UTF-8</OutputEncoding>
	  <InputEncoding>UTF-8</InputEncoding>
	
</OpenSearchDescription>
